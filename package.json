{
  "name": "jest-fp-ts",
  "version": "1.0.0",
  "description": "Additional Jest matchers for projects using using fp-ts and the fp-ts ecosystem",
  "main": "index.js",
  "keywords": [
    "jest",
    "matchers",
    "fp-ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relmify/jest-fp-ts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relmify/jest-fp-ts/issues"
  },
  "homepage": "https://github.com/relmify/jest-fp-ts#readme",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "clean": "npm-run-all clean:**",
    "clean:out": "rimraf out",
    "clean:jest-cache": "jest --clear-cache --bail 0",
    "check-lint": "eslint ./src --ext .ts",
    "check-prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "testwatch": "jest --watch"
  },
  "dependencies": {
    "expect": "^25.5.0",
    "fp-ts": "2.x",
    "io-ts": "2.x",
    "jest-get-type": "^25.2.6",
    "jest-matcher-utils": "^25.5.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "jest-extended": "^0.11.5",
    "jest-watch-toggle-config": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
